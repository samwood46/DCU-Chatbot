<aiml version="2.0" encoding="UTF-8">
<!-- basic_chat.aiml -->

    <!-- different ways the user can be provided with information about a bus -->
    <category>
        <pattern>BUS</pattern>
        <template>There is a number of buses from the College./n Would you like to see a list?</template>
    </category>

    <category>
        <pattern>* BUS *</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>* BUS</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>BUS *</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>BUSES</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>* BUSES *</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>BUSES *</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>* BUSES</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

    <category>
        <pattern>* BUSES *</pattern>
        <template>
            <srai>BUS</srai>
        </template>
    </category>

